						<AbsDimension x="64" y="24" />
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset x="32" y="-180" />
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>wpFrame_OnLoad();</OnLoad>
			<OnEvent>wpFrame_OnEvent();</OnEvent>
			<OnShow>wpFrame_OnShow();</OnShow>
		</Scripts>
	</Frame>
</Ui>
