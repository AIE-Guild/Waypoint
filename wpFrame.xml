				</Anchors>
				<Scripts>
					<OnClick>wpMsgSaveButton_OnClick();</OnClick>
					<OnShow>wpMsgSaveButton_OnShow();</OnShow>
				</Scripts>
				<NormalFont style="GameFontNormal" />
