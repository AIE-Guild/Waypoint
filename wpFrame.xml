				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset x="32" y="-424" />
					</Anchor>
				</Anchors>
			</Button>
			<Button name="wpConNameSortButton" inherits="UIPanelButtonTemplate" text="Name">
				<Size>
					<AbsDimension x="128" y="24" />
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset x="102" y="-180" />
